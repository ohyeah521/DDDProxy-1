<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.wangdongxu.dddproxy</string>
    <key>RunAtLoad</key><true/>
    <key>KeepAlive</key><false/>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python</string>
        <string>localServer.py</string>
    </array>
    <key>WorkingDirectory</key><string>path-to-DDDProxy</string>
	<key>StandardErrorPath</key><string>/tmp/ddproxy.err.log</string>
	<key>StandardOutPath</key><string>/tmp/ddproxy.out.log</string>
  </dict>
</plist>
